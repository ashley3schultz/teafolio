<%= form_for @tea do |f| %>
    <h2><%= f.label :name %>
    <%= f.text_field :name %></h2>
    <h2><%= f.label :alternate_name %>
    <%= f.text_field :aka %></h2>
    <h2><%= f.label :type %>
    <%= f.text_field :oxidation %></h2>
    <h2><%= f.label :description %>
    <%= f.text_field :description %></h2>
    <%= f.submit :submit %>
<% end %>